{
  "mcpServers": {
    "TmarAITools": {
      "command": "${WORKSPACE_FOLDER}/node_modules/.bin/vf",
      "args": ["mcp"],
      "env": {
        "WORKSPACE_FOLDER": "${WORKSPACE_FOLDER}"
      }
    }
  },
  "permissions": {
    "allow": [],
    "deny": [
      "Task"
    ],
    "ask": [
      "Bash(rm:*)",
      "Bash(kill:*)",
      "Bash(chmod:*)"
    ]
  },
  "env": {}
}
