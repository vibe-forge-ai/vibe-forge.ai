{
  "name": "@vibe-forge/adapter-claude-code",
  "version": "0.0.1",
  "description": "Claude Code Adapter for Vibe Forge",
  "bin": {
    "claude": "./node_modules/.bin/claude"
  },
  "exports": {
    ".": {
      "__vibe-forge__": {
        "default": "./src/index.ts"
      },
      "default": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    }
  },
  "dependencies": {
    "@vibe-forge/core": "workspace:^",
    "@anthropic-ai/claude-code": "2.1.27",
    "@musistudio/claude-code-router": "1.0.43"
  }
}
