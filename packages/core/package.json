{
  "name": "@vibe-forge/core",
  "version": "0.1.0",
  "imports": {
    "#~/*": {
      "__vibe-forge__": {
        "default": "./src/*.ts"
      },
      "default": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    }
  },
  "exports": {
    ".": {
      "__vibe-forge__": {
        "default": "./src/index.ts"
      },
      "default": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    }
  }
}
